{
  "$schema": "https://developer.microsoft.com/json-schemas/tsdoc/v0/tsdoc.schema.json",
  "extends": [
    "@microsoft/tsdoc/tsdoc-metadata.json"
  ],
  "tagDefinitions": [
    {
      "tagName": "@fileOverview",
      "syntaxKind": "block",
      "allowMultiple": false
    },
    {
      "tagName": "@mermaid",
      "syntaxKind": "block",
      "allowMultiple": true
    }
  ],
  "supportForTags": {
    "@fileOverview": true,
    "@mermaid": true
  },
  "reportUnsupportedHtmlElements": false
}
